<?xml version="1.0"?>
<launch>

  <node respawn="false" pkg="viconxbee" type="viconxbee_node" name="viconxbee_node">
	<param name="viconSerialPort" type="string" value="/dev/sensors/ftdi_AM01P5H0" /> <!-- Xbee Id make A7<-A4 -->
	<param name="viconNodeRate" type="int" value="50"/>
	<param name="viconCommTimeout" type="int" value="250"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="vicon_to_local_origin_tf" 
        args="0 0 0 2.40733333 0 0 vicon local_origin 10" />
  <node pkg="tf" type="static_transform_publisher" name="local_origin_to_ned_tf" 
        args="0 0 0 1.5707963265 0 3.141592653 local_origin ned 10" />
  
</launch>
